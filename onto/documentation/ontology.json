}, {
  "@id" : "http://webprotege.stanford.edu/project/0xQQAXy4OncaIW5CUWUbM",
  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A location, in a very generic sense: a political geographic entity (Roma, Lesotho), a location determined by the presence of other entities ('the area close to Roma'), pivot events or signs (\"the area where the helicopter fell\"), complements of other entities ('the area under the table'), as well as physical objects conceptualized as locations as their main identity criterion ('the territory of Italy').\nIn this generic sense, a Place is an 'approximate', relative location. For an 'absolute', abstract location, cf. the pattern spaceregion.owl.\nFormally, a Place is defined by the fact of having something located in it; a Place is located in itself."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "it",
    "@value" : "Luogo"
  }, {
    "@language" : "en",
    "@value" : "Place"
  } ],
  "http://www.w3.org/2002/07/owl#equivalentClass" : [ {
    "@id" : "_:genid5"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#TransitiveProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A generic, relative localization, holding between any entities. E.g. 'the cat is on the mat', 'Omar is in Samarcanda', 'the wound is close to the femural artery'.\nFor 'absolute' locations, see SpaceRegion"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "it",
    "@value" : "ha localizzazione"
  }, {
    "@language" : "en",
    "@value" : "has location"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subPropertyOf" : [ {
    "@id" : "http://w3id/gicentre/onto#R9hKF6kdYUCv5SDIsmIwkNK"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#isLocationOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#TransitiveProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A generic, relative localization, holding between any entities. E.g. 'Rome is the seat of the Pope', 'the liver is the location of the tumor'.\nAs a very general notion of localization, it is assumed as transitive.\nFor 'absolute' locations, see SpaceRegion"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#Place"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "is location of"
  }, {
    "@language" : "it",
    "@value" : "Ã¨ una localizzazione di"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "http://www.ontologydesignpatterns.org/cp/owl/place.owl#hasLocation"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used for exemplifying possible requirements the content pattern provides a solution for. Requirements are expressed as natural language competency questions."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#extractedFrom",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property should be assigned with a URI, which points to the possible reference ontology which the annotated pattern was extracted from (i.e. the reference ontology that the ontology elements have been deeply or partially cloned by). The range is not explicit in the definition of the annotation property because it would turn the ontology to OWL Full. E.g. The participation pattern is extracted from the Dolce Ultra Lite ontology, hence the value for this annotation property is http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasAuthor",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasComponent",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is useful for content ontology design patterns. Its value is a URI, which refers to another content ontology design pattern which is a component of the annotated one."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasConsequences",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used for briefly describing the benefits and/or possible trade-offs when using the CP."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasIntent",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used in order to describe the intent of the content pattern."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#hasUnitTest",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This property can be used to annotate a unit test (e.g. in the form of a SPARQL query) to be launched to evaluate an ontology against a requirement-based task."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#isCloneOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used for referring a cloned ontology entity to its cloning source."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#isSpecializationOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is useful for content ontology design patterns and its elements. Its value is a URI, which refers either to a content ontology design pattern that is specialized by the annotated one, or to an ontology element that is specialized by the annotated one."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#reengineeredFrom",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property should be assigned with a URI, which points to the concept schema, page, or anything else from which the annotated pattern was reengineered.  If the source does not have any URI e.g., a printed book, this property value should provide information as precise as possible in order to identify the source. This property is alternative to the extractedFrom property because it is used when the pattern come from a concept schema which is not an owl ontology. For example content ontology design patterns, which are reengineered from data model patterns, rdf schemas, etc. should be annotatd with this property. E.g. The basicpersonalfoaf pattern is extracted from the rdf FOAF specification, hence the value for this annotation property is http://xmlns.com/foaf/spec/20071002.rdf"
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#relatedCPs",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property can be used to indicate other CPs (if any) that specialize, generalize, inlcude, or are components of the CP. Furthermore, this field may indicate other CPs that are typically used in conjunction with the described one. Important similarities and differences with other patterns can be also described here."
  } ]
}, {
  "@id" : "http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#scenarios",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used for describing examples of instantiation of the Content OP. For example, for the part-of Content OP (which represents part-whole relations) a possible scenario is the sentence: \"the brain is part of the human body\". Scenarios are expressed as natural language sentences."
  } ]
}, {
  "@id" : "http://xd-protege.com/schemas/cpas-ext.owl",
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This is an extension to the existing Content ODP annotation schema adding further needed annotation properties used by XDP."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@id" : "http://xd-protege.com"
  } ],
  "http://www.w3.org/2002/07/owl#versionInfo" : [ {
    "@value" : "Created with TopBraid Composer"
  } ]
}, {
  "@id" : "http://xd-protege.com/schemas/cpas-ext.owl#category",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used in order to list the domains or categories in which a content ontology design pattern is intended to be used."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "category"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"
  } ]
}, {
