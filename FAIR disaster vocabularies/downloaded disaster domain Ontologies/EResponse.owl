		<rdfs:range rdf:resource="&EResponse;Victim_status"/>
	</owl:ObjectProperty>
	

    <!-- http://ontology.eil.utoronto.ca/tove/activity.owl#sub_state -->

    <owl:ObjectProperty rdf:about="&act;sub_state">
		<rdfs:domain rdf:resource="&EResponse;EmergencyRequest"/>
		<rdfs:range>
			 <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&EResponse;RequestByCall"/>
					<rdf:Description rdf:about="&EResponse;RequestByText"/>
                </owl:unionOf>
            </owl:Class>
		</rdfs:range>
	</owl:ObjectProperty>
	

