  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used in order to describe the solution description of a content ontology design pattern."
  } ],
