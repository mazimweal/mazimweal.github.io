    <owl:Class rdf:about="http://w3id.org/gicentre/onto/eventTypeFull#EventType">
        <owl:disjointWith rdf:resource="http://w3id.org/gicentre/onto/eventTypeFull#Geometry"/>
        <owl:disjointWith rdf:resource="http://w3id.org/gicentre/onto/eventTypeFull#Parameter"/>
