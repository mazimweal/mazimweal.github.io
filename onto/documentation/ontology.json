  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "This annotation property is used in order to indicate where a graphical illustration of a content ontology design pattern may be found."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
