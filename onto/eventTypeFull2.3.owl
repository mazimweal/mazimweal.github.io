            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/gicentre/onto/eventTypeFull#occursAtPlace"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/gicentre/onto/eventTypeFull#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/gicentre/onto/eventTypeFull#occursAtTime"/>
